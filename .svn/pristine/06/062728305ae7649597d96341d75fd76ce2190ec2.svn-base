.widget-reacttour-clickable {
    cursor: pointer;
}

.widget-reacttour {
    display: inline-block;
}

.widget-reacttour.badge:empty {
    display: initial;
    /* Fix padding to stay round */
    padding: 3px 10px;
}

.widget-reacttour.label:empty {
    display: initial;
    /* Fix padding to stay square */
    padding: .2em .8em .3em;
}

.widget-reacttour.badge {
    min-width: 18px;
    min-height: 18px;
}

